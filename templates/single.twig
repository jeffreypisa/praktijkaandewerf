{% extends "base.twig" %}

{% block content %}
	<div class="content-wrapper">
		<article class="post-type-{{ post.post_type }}" id="post-{{ post.ID }}">
			<div class="container-fluid">
				<h2 class="h1 mt-4"> {{ post.title }} </h2>
				<div class="line"></div>
				<div class="row">
					<div class="col-md">	
						{{ post.kolom_1 }}
					</div>
					<div class="col-md">	
						{{ post.kolom_2 }}
					</div>
				</div>
			</div>
		</article>
	</div><!-- /content-wrapper -->
{% endblock %}
